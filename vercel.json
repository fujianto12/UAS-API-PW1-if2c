{
   "version": 2,
   "name": "public-api",
   "builds": [
     { "src": "server.js", "use": "@vercel/node" }
   ],
   "routes": [
     { "src": "/(.*)", "dest": "/server.js" }
   ],
   "headers": [
     {
       "source": "/products", 
       "headers": [
         {
           "key": "Access-Control-Allow-Origin",
           "value": "http://localhost:5173" 
         }
       ]
     }
   ]
 }
 